@model ROM.Models.MemberVM
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Add</title>
</head>
<body>
    <div>
        <div class="form-group">
            @*<label class="control-label col-sm-2" for="Name">Name:</label>*@
            @Html.LabelFor(c => c.Code, null, new { @class = "control-label col-sm-2" })
            <div class="col-sm-10">
                @*<input type="text" class="form-control" id="Name" name="Name" placeholder="Name">*@
                @Html.TextBoxFor(c => c.Code, null, new { @class = "form-control", placeholder = "Code" })
                @*@Html.ValidationMessageFor(c=>c.Name,null,new {@class="text-danger"})*@
            </div>
        </div>
        <div class="form-group">
            @Html.LabelFor(c => c.Name, null, new { @class = "control-label col-sm-2" })
            <div class="col-sm-10">
                @Html.TextBoxFor(c => c.Name, null, new { @class = "form-control", placeholder = "Name" })
            </div>
        </div>
        <div class="form-group">
            @Html.LabelFor(c => c.Email, null, new { @class = "control-label col-sm-2" })
            <div class="col-sm-10">
                @Html.TextBoxFor(c => c.Email, null, new { @class = "form-control", placeholder = "Email" })
            </div>
        </div>
        <div class="form-group">
            @Html.LabelFor(c => c.ContactNo, null, new { @class = "control-label col-sm-2" })
            <div class="col-sm-10">
                @Html.TextBoxFor(c => c.ContactNo, null, new { @class = "form-control", placeholder = "ContactNo" })
            </div>
        </div>
        <div class="form-group">
            <div class="col-sm-offset-2 col-sm-10">
                <button type="submit" class="btn btn-success" value="Save">Save</button>
            </div>
        </div>

    </div>
</body>
</html>
